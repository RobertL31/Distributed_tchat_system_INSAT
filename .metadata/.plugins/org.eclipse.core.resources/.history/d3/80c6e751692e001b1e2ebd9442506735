package distantApp;

import java.net.Inet4Address;
import java.util.HashMap;

public class NetworkManager {
	
	private HashMap<String, Inet4Address > m_IP_Pseudo_Table;
	private Network_Sender m_sender;
	private Network_receiver m_receiver;
	
	
	public UsersManager(HashMap<String, Inet4Address> m_IP_Pseudo_Table, Network_Sender m_sender,
			Network_receiver m_receiver) {
		super();
		this.m_IP_Pseudo_Table = m_IP_Pseudo_Table;
		this.m_sender = m_sender;
		this.m_receiver = m_receiver;
	}
	 
	 
	 
	 

}
